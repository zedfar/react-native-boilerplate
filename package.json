{
  "name": "expo-boilerplate",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "server": "json-server --watch server/db.json --host 0.0.0.0 --port 3001 --middlewares server/middleware.js --routes server/routes.json",
    "dev": "concurrently -n expo,server -c blue,green \"expo start --no-interactive --clear\" \"npm run server\"",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "babel-preset-expo": "^54.0.5",
    "expo": "~54.0.0",
    "expo-constants": "~18.0.9",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "^6.0.12",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "lucide-react-native": "^0.546.0",
    "nativewind": "^4.0.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "^0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-worklets": "0.5.1",
    "expo-local-authentication": "~17.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~19.1.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "concurrently": "^8.2.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "json-server": "^0.17.4",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.9.2"
  },
  "private": true,
  "expo": {
    "install": {
      "exclude": [
        "react",
        "expo",
        "expo-constants",
        "react-native"
      ]
    }
  }
}
